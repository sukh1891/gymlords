<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ktech.gymlrds" version="1.3.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
    <name>GymLords Fitness</name>
    <description>
        The official app of GymLords Fitness. Get regular workout and nutrition tips directly on your phone.
    </description>
    <author email="budget@khuranatech.in" href="http://budget.khuranatech.in">
        Khurana Technologies
    </author>
    <content src="index.html" />
    <preference name="orientation" value="portrait"/>
    <preference name="fullscreen" value="false"/>
    <preference name="target-device" value="universal"/>
    <preference name="webviewbounce" value="true"/>
    <preference name="prerendered-icon" value="true"/>
    <preference name="stay-in-webview" value="false"/>
    <preference name="show-splash-screen-spinner" value="false"/>
    <preference name="detect-data-types" value="true"/>
    <preference name="exit-on-suspend" value="false"/>
    <preference name="disable-cursor" value="false"/>
    <preference name="android-minSdkVersion" value="14"/>
    <preference name="android-installLocation" value="auto"/>
    <preference name="AutoHideSplashScreen" value="true"/>
    <plugin name="cordova-plugin-device-orientation" />
    <plugin name="cordova-plugin-network-information" />
    <plugin name="cordova-plugin-splashscreen" />
    <plugin name="cordova-plugin-device" />
    <plugin name="cordova-plugin-local-notifications" />
    <access origin="*" />
    <plugin name="cordova-plugin-whitelist" version="1"/>
    <allow-intent href="http://*/*"/>
    <allow-intent href="https://*/*"/>
    <allow-intent href="mailto:*"/>
    <allow-intent href="whatsapp://*/*"/>
    <platform name="android">
        <allow-intent href="market:*"/>
        <icon src="www/img/icon.png"/>
        <splash src="www/img/app-splash.png" density="mdpi"/>
    </platform>
</widget>